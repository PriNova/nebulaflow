{
    "compilerOptions": {
        "target": "ES2023",
        "lib": ["ES2023", "DOM", "DOM.Iterable"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "types": ["vite/client"],
        "noUnusedLocals": false
    },
    "include": ["**/*.ts", "**/*.tsx", "workflow.html"]
}
